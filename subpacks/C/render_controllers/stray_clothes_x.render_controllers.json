{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.stray_clothes_x": {
      "rebuild_animation_matrices": true,
      "geometry": "Geometry.overlay",
      "materials": [
        {
          "*": "Material.overlay"
        }
      ],
      "textures": [
        "Texture.overlay"
      ],
      "overlay_color": {
        "r": "(variable.Avar>0 ? 1.0 : -1.0)",
        "g": "(variable.Avar>0 ? 1.0 : -1.0)",
        "b": "(variable.Avar>0 ? 1.0 : -1.0)",
        "a": "math.abs(variable.Avar)/20*0.15"
      }
    }
  }
}