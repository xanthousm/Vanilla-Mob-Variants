{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.shulker_x": {
      "arrays": {
        "textures": {
          "Array.variants": [
            "Texture.black",
            "Texture.red",
            "Texture.green",
            "Texture.brown",
            "Texture.blue",
            "Texture.purple",
            "Texture.cyan",
            "Texture.silver",
            "Texture.gray",
            "Texture.pink",
            "Texture.lime",
            "Texture.yellow",
            "Texture.light_blue",
            "Texture.magenta",
            "Texture.orange",
            "Texture.white",
            "Texture.undyed"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "Array.variants[query.variant]"
      ],
      "overlay_color": {
        "r": "(variable.Avar>0 ? 1.0 : -1.0)",
        "g": "(variable.Avar>0 ? 1.0 : -1.0)",
        "b": "(variable.Avar>0 ? 1.0 : -1.0)",
        "a": "math.abs(variable.Avar)/20*0.15"
      }
    }
  }
