{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.phantom_x": {
      "arrays": {
        "materials": {
          "Array.materials": [
            "Material.default",
            "Material.invisible"
          ]
        },
        "textures": {
          "Array.Xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4",
            "Texture.default5",
            "Texture.default6",
            "Texture.default7",
            "Texture.default8",
            "Texture.default9",
            "Texture.default10",
            "Texture.default13",
            "Texture.default14",
            "Texture.default15",
            "Texture.default16",
            "Texture.default17",
            "Texture.default18",
            "Texture.default19",
            "Texture.default20",
            "Texture.default21"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Array.materials[query.is_invisible]"
        }
      ],
      "textures": [
        "Array.Xskins[variable.Xvar]"
      ],
      "overlay_color": {
        "r": "(variable.Avar>0 ? 1.0 : -1.0)",
        "g": "(variable.Avar>0 ? 1.0 : -1.0)",
        "b": "(variable.Avar>0 ? 1.0 : -1.0)",
        "a": "math.abs(variable.Avar)/20*0.15"
      }
    }
  }
}