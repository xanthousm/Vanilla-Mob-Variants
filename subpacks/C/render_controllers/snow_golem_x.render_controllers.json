{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.snowgolem_x": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default"
          ],
          "Array.Xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4",
            "Texture.default5",
            "Texture.default6",
            "Texture.default7",
            "Texture.default8",
            "Texture.default9",
            "Texture.default10",
            "Texture.default20",
            "Texture.default21",
            "Texture.default22",
            "Texture.default23",
            "Texture.default24",
            "Texture.default25",
            "Texture.default26",
            "Texture.default27",
            "Texture.default28"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        },
        {
          "head*": "Material.head"
        }
      ],
      "textures": [
        "query.get_name == 'Frank' ? Texture.Frank : (Array.Xskins[variable.Xvar])"
      ],
      "overlay_color": {
        "r": "(variable.Avar>0 ? 1.0 : -1.0)",
        "g": "(variable.Avar>0 ? 1.0 : -1.0)",
        "b": "(variable.Avar>0 ? 1.0 : -1.0)",
        "a": "math.abs(variable.Avar)/20*0.15"
      }
    }
  }
}